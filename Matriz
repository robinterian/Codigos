#include<stdio.h>

main()
{
	int i,j,m[3][3], a[3][3], b[3][3];
	for(i=0;i<3;i++)
	 for(j=0;j<3;j++)
 {
 	printf("Numero a:\t"); scanf("%d", &a[i][j]);
 	printf("Numero b:\t"); scanf("%d", &b[i][j]);
 	m[i][j]=a[i][j]+b[i][j];
 }
 for(i=0;i<3;i++)
 {
 	for(j=0;j<3;j++)
 	{
 		printf(" %d", m[i][j]);
	 }
	 printf("\n");
 }
}
